{
  "multipart": [
    {
      "apply": {
        "model": "immersiveposts:leadpost/post"
      },
      "when": {
        "type": "post"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/post_top"
      },
      "when": {
        "type": "post_top"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/post_arm"
      },
      "when": {
        "parm_north": "true"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/post_arm",
        "y": 90
      },
      "when": {
        "parm_east": "true"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/post_arm",
        "y": 180
      },
      "when": {
        "parm_south": "true"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/post_arm",
        "y": 270
      },
      "when": {
        "parm_west": "true"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm"
      },
      "when": {
        "type": "arm",
        "facing": "north",
        "flipstate": "up"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_double"
      },
      "when": {
        "type": "arm_double",
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "y": 90
      },
      "when": {
        "type": "arm",
        "facing": "east",
        "flipstate": "up"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_double",
        "y": 90
      },
      "when": {
        "type": "arm_double",
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "y": 180
      },
      "when": {
        "type": "arm",
        "facing": "south",
        "flipstate": "up"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_double",
        "y": 180
      },
      "when": {
        "type": "arm_double",
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "y": 270
      },
      "when": {
        "type": "arm",
        "facing": "west",
        "flipstate": "up"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_double",
        "y": 270
      },
      "when": {
        "type": "arm_double",
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "x": 180,
        "y": 180
      },
      "when": {
        "type": "arm",
        "facing": "north",
        "flipstate": "down"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "x": 180,
        "y": 270
      },
      "when": {
        "type": "arm",
        "facing": "east",
        "flipstate": "down"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "x": 180
      },
      "when": {
        "type": "arm",
        "facing": "south",
        "flipstate": "down"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm",
        "x": 180,
        "y": 90
      },
      "when": {
        "type": "arm",
        "facing": "west",
        "flipstate": "down"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_twoway"
      },
      "when": {
        "type": "arm",
        "facing": "north",
        "flipstate": "both"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_twoway",
        "y": 90
      },
      "when": {
        "type": "arm",
        "facing": "east",
        "flipstate": "both"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_twoway",
        "y": 180
      },
      "when": {
        "type": "arm",
        "facing": "south",
        "flipstate": "both"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:leadpost/arm_twoway",
        "y": 270
      },
      "when": {
        "type": "arm",
        "facing": "west",
        "flipstate": "both"
      }
    },
    {
      "apply": {
        "model": "immersiveposts:block/empty"
      },
      "when": {
        "type": "empty"
      }
    }
  ]
}